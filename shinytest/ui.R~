library(shiny)

# Define UI for application that draws a histogram
shinyUI(fluidPage(

  # Application title
  titlePanel("PhyAdequacy"),

  sidebarLayout(
    sidebarPanel(
	h4("Welcome to PhyAdequacy"),
	p("This software assesses the adequacy of models used in phylogenetics.")

    ),
    mainPanel("main panel")
  )
))